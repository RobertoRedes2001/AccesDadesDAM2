<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida.tema3">
	<class name="Personatge" table="fighter">
		<id name="id" column="ID">
		<generator class="increment"/>
		</id>
		<property name="name" type="string" column="CharName"/>
		<property name="title" type="string" column="Title"/>
		<property name="poweruds" type="int" column="PowerUds"/>
		<property name="element" type="string" column="Element"/>
	</class>
</hibernate-mapping>